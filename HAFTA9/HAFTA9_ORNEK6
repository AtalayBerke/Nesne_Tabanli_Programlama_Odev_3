using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Ornek_6
{
    interface IArayuz
    {
        int metot1();
        void metot2();

        int sayi
        {
            get;
            set;
        }

        int this[int indeks]
        {
            get;
        }
        
    }

    class Sinif : IArayuz
    {
        private int sayi;
        public int metot1()
        {
            return 0;
        }

        public void metot2()
        {

        }

        public int Sayi
        {
            get { return sayi; }
            set { sayi = value; }
        }

        public int this[int indeks]
        {
            get { return indeks; }
            set { }
        }
    }

    class Program
    {
        static void Main(string[] args)
        {

            Sinif s = new Sinif();
        }
    }
}
